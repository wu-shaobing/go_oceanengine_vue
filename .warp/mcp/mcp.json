{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp"]
    },
    "neon": {
      "command": "npx",
      "args": ["-y", "@neondatabase/mcp-server-neon"],
      "env": {
        "NEON_API_KEY": ""
      }
    },
    "supabase-memory": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server-supabase"],
      "env": {
        "SUPABASE_URL": "",
        "SUPABASE_KEY": ""
      }
    },
    "figma": {
      "command": "npx",
      "args": ["-y", "figma-mcp"]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "mem0-memory": {
      "command": "npx",
      "args": ["-y", "@mem0/mcp"],
      "env": {
        "MEM0_API_KEY": ""
      }
    },
    "replicate": {
      "command": "npx",
      "args": ["-y", "replicate-mcp"],
      "env": {
        "REPLICATE_API_TOKEN": ""
      }
    },
    "vercel-weather": {
      "command": "npx",
      "args": ["-y", "@iflow-mcp/weather-mcp"]
    },
    "perplexity": {
      "command": "npx",
      "args": ["-y", "perplexity-mcp"],
      "env": {
        "PERPLEXITY_API_KEY": ""
      }
    },
    "cloudflare": {
      "command": "npx",
      "args": ["-y", "@cloudflare/mcp-server-cloudflare"],
      "env": {
        "CLOUDFLARE_API_TOKEN": "",
        "CLOUDFLARE_ACCOUNT_ID": ""
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "github-mcp-custom", "stdio"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    },
    "stripe": {
      "command": "npx",
      "args": ["-y", "@stripe/mcp", "--tools=all"],
      "env": {
        "STRIPE_SECRET_KEY": ""
      }
    },
    "databutton": {
      "command": "npx",
      "args": ["-y", "@iflow-mcp/databutton-mcp"]
    },
    "semgrep": {
      "command": "npx",
      "args": ["-y", "mcp-server-semgrep"]
    },
    "semantic-scholar": {
      "command": "npx",
      "args": ["-y", "researchmcp"]
    }
  }
}


