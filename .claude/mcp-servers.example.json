{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/Users/wushaobing911/Desktop/douyin"]
    },
    "grep": {
      "command": "npx",
      "args": ["-y", "grep-mcp"]
    },
    "duckduckgo": {
      "command": "npx",
      "args": ["-y", "duckduckgo-mcp-server"]
    },
    "sqlite": {
      "command": "npx",
      "args": ["-y", "mcp-sqlite-tools"]
    },
    "ollama-local": {
      "command": "npx",
      "args": ["-y", "github:patruff/ollama-mcp-bridge"]
    },
    "postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres"]
    },
    "browser": {
      "command": "npx",
      "args": ["-y", "playwright-mcp"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp"]
    },
    "figma": {
      "command": "npx",
      "args": ["-y", "figma-mcp"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "github-mcp-custom", "stdio"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "stripe": {
      "command": "npx",
      "args": ["-y", "@stripe/mcp", "--tools=all"],
      "env": {
        "STRIPE_SECRET_KEY": "${STRIPE_KEY}"
      }
    },
    "databutton": {
      "command": "npx",
      "args": ["-y", "@iflow-mcp/databutton-mcp"]
    },
    "semgrep": {
      "command": "npx",
      "args": ["-y", "mcp-server-semgrep"]
    },
    "semantic-scholar": {
      "command": "npx",
      "args": ["-y", "researchmcp"]
    },
    "vercel-weather": {
      "command": "npx",
      "args": ["-y", "@iflow-mcp/weather-mcp"]
    }
  }
}
